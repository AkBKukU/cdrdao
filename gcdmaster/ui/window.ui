<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GtkApplicationWindow" id="app_window">
    <property name="default-width">350</property>
    <property name="default-height">250</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="header">
        <property name="title" translatable="yes">Gnome CD Master</property>
        <property name="visible">True</property>
        <property name="show-close-button">True</property>
        <property name="decoration-layout">menu:close</property>
        <child>
          <object class="GtkButton" id="blank_button">
            <property name="tooltip-text">Blank CD-RW</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="border_width">0</property>
            <property name="relief">none</property>
            <property name="image">blank_img</property>
            <property name="action-name">app.blank-cdrw</property>
          </object>
          <packing>
            <property name="pack_type">end</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkMenuButton" id="gears">
            <property name="visible">True</property>
            <property name="direction">none</property>
            <property name="use-popover">True</property>
            <style>
              <class name="image-button"/>
            </style>
          </object>
          <packing>
            <property name="pack-type">end</property>
          </packing>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkNotebook" id="notebook">
        <property name="visible">True</property>
      </object>
    </child>
  </object>
  <object class="GtkImage" id="blank_img">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="resource">/org/gnome/gcdmaster/blank.png</property>
  </object>
 </interface>
